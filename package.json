{
    "name": "strictly",
    "displayName": "strictly",
    "publisher": "strictly",
    "description": "strictly langauge support",
    "version": "0.0.0-semantically-released",
    "scripts": {
        "test": "echo \"no test specified\""
    },
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "strictly",
                "aliases": [
                    "strictly",
                    "strictly"
                ],
                "extensions": [
                    ".sly"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "strictly",
                "scopeName": "source.strictly",
                "path": "./syntaxes/strictly.tmLanguage.json"
            }
        ]
    },
    "author": "Carlo Jeske",
    "license": "MIT",
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.1",
        "@semantic-release/commit-analyzer": "^9.0.1",
        "@semantic-release/exec": "^6.0.2",
        "@semantic-release/github": "^8.0.2",
        "@semantic-release/npm": "^8.0.3",
        "@semantic-release/release-notes-generator": "^10.0.2",
        "eslint": "^8.3.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.4.1",
        "semantic-release": "^18.0.0",
        "vsce": "^2.10.0"
    },
    "release": {
        "branches": [
            "main"
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            [
                "@semantic-release/npm",
                {
                    "npmPublish": false
                }
            ],
            [
                "@semantic-release/exec",
                {
                    "publishCmd": "npx vsce publish --yarn"
                }
            ]
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/strictly-lang/vscode-plugin.git"
    }
}
