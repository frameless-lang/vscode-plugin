{
    "name": "strictly",
    "displayName": "strictly",
    "description": "strictly langauge support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "strictly",
                "aliases": [
                    "strictly",
                    "strictly"
                ],
                "extensions": [
                    ".sly"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "strictly",
                "scopeName": "source.strictly",
                "path": "./syntaxes/strictly.tmLanguage.json"
            }
        ]
    },
    "author": "Carlo Jeske",
    "license": "MIT"
}